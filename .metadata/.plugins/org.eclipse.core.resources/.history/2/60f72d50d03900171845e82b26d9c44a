package tapofwar.newpackage;
import java.awt.*;

import javax.swing.ImageIcon;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JPanel;


public class TestSwing3 extends JFrame {
    
    public static void main(String[] args) {    
        new TestSwing3().start();
        Frame aFrame = new Frame("The cat and the fiddle");
        aFrame.add(new TextField("Text To Display"));
        aFrame.setSize(400, 100);
        aFrame.setVisible(true);
    }
    
    public void start(){
    	ImageImplement panel = new ImageImplement(new ImageIcon("src/bigseashore.png").getImage());
    	add(panel);
        JLabel label = new JLabel("hi");
    	panel.add(label);
    	setVisible(true);
    	setSize(1000,550);
    	setDefaultCloseOperation(EXIT_ON_CLOSE);
    }
}